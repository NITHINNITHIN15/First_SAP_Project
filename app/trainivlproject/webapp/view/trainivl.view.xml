<mvc:View controllerName="trainivlproject.controller.trainivl"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page" title="Employee List">
        <Table id="_IDGenTable" items="{employeeModel>/employees}">
            <columns>
                <Column id="_IDGenColumn1">
                    <Text id="_IDGenText" text="Name" />
                </Column>
                <Column id="_IDGenColumn2">
                    <Text id="_IDGenText1" text="Designation" />
                </Column>
                <Column id="_IDGenColumn3">
                    <Text id="_IDGenText2" text="Email" />
                </Column>
            </columns>
            <items>
                <ColumnListItem id="_IDGenColumnListItem">
                    <cells>
                        <Text id="_IDGenText3" text="{employeeModel>name}" />
                        <Text id="_IDGenText4" text="{employeeModel>designation}" />
                        <Text id="_IDGenText5" text="{employeeModel>email}" />
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </Page>
</mvc:View>
